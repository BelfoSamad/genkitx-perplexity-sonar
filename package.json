{
  "name": "genkitx-perplexity-sonar",
  "description": "Firebase Genkit AI framework plugin for Perplexity Sonar APIs.",
  "keywords": [
    "genkit",
    "genkit-plugin",
    "genkit-model",
    "perplexity",
    "sonar",
    "ai",
    "genai",
    "generative-ai"
  ],
  "version": "0.0.1",
  "type": "commonjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BelfoSamad/genkitx-perplexity-sonar.git"
  },
  "author": "SamadTch",
  "license": "Apache-2.0",
  "main": "index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.12.12",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
